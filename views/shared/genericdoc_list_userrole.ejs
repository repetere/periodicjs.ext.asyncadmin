<details class="ts-genericdoc">
  <summary>
    <div class="ts-genericdoc-hoveroptions ts-pull-right">
      <a href="/<%- locals.adminPath %>/<%- genericdoctypelink %>/<%- genericdoc._id %>/edit" class="async-admin-ajax-link"><img src="/extensions/periodicjs.ext.asyncadmin/img/icons/doc_edit_three.svg" alt="edit"  class="ts-icon async-admin-ajax-link" data-ajax-href="/<%- locals.adminPath %>/<%- genericdoctypelink %>/<%- genericdoc._id %>/edit" /></a>
      <!-- <img src="/extensions/periodicjs.ext.asyncadmin/img/icons/doc_view_two.svg" alt="view"  class="ts-icon" /></a> -->
      <a><img src="/extensions/periodicjs.ext.asyncadmin/img/icons/doc_delete.svg" alt="delete"  class="ts-icon ts-button-error-color ts-dialog-delete"  data-href="/<%- locals.adminPath %>/<%- genericdoctypelink %>/<%- genericdoc._id %>/delete" data-deleted-redirect-href="/<%- locals.adminPath %>/<%- genericdoctypeplurallink %>"/></a>
    </div>
    <%- genericdoc.title %>
    <small class="ts-text-divider-text-color">(<%- genericdoc.userroleid %>) 
    </small>

    <hr>
  </summary>
  <aside class="ts-text-xs">
    <div class="ts-row">
      <div class="ts-screensm-col-span12 ts-col-span12 ">
        <strong>User Privileges</strong>:
        <%- genericdoc.privileges %>
        <% generictaxomony = genericdoc.privileges %>
        <% include generictax_list_item %>
      </div>
    </div>
    <div class="ts-row">
      <div class="ts-col-span12">
        <strong>Name</strong>:  <%- genericdoc.name %> 
      </div>
      <div class="ts-col-span12">
        <strong>Description</strong>:  <%- genericdoc.description %> 
      </div>
    </div>
  </aside>
</details>